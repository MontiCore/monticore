grammar ExtendingNT2 extends de.monticore.MCBasics {

// ----------------------- Extension of NTs

  A = "...1" | B;
  B = "...2";
  C = A;

// ----------------------- Extension of NTs

  AutomatonElement = State | Transition;

  State = "...1" ;

  Transition = "...1" ;

}

