grammar B10 extends A {

  // M is adapted and reused, but its cardinality adapted

  // Orig:  M = Decimal; 
  M = "sic" Integer New* N P Q "end";

  // Orig:  N = "one" M;
  N = "ein" M* "ende";

  // Orig:  P = "some" M*;
  P = "einige" M "ende";

  // Orig:  Q = "optional" M?;
  Q = "opti" M*;

  New = Integer;
}
